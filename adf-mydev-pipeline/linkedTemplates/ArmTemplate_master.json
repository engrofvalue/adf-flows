{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-mydev-pipeline"
		},
		"LS_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS'"
		},
		"LS_Azure_Sql_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Azure_Sql_DB'"
		},
		"LS_Sql_Db_Az_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Sql_Db_Az'"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfstorage0123.dfs.core.windows.net/"
		},
		"LS_Azure_Sql_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "db-server-0630.database.windows.net"
		},
		"LS_Azure_Sql_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SQL_DB1"
		},
		"LS_Azure_Sql_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"LS_Http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://drive.usercontent.google.com/u/0/uc?id=1bmmtkc5NLZQ4iU6Qz_7di-juuxA2kT1t&export=download"
		},
		"LS_Rest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reqres.in/api/users"
		},
		"LS_Sql_Db_Az_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "db-server-0630.database.windows.net"
		},
		"LS_Sql_Db_Az_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "az_db"
		},
		"LS_Sql_Db_Az_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-mydev-pipeline_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_accountKey": {
						"value": "[parameters('LS_ADLS_accountKey')]"
					},
					"LS_Azure_Sql_DB_password": {
						"value": "[parameters('LS_Azure_Sql_DB_password')]"
					},
					"LS_Sql_Db_Az_password": {
						"value": "[parameters('LS_Sql_Db_Az_password')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					},
					"LS_Azure_Sql_DB_properties_typeProperties_server": {
						"value": "[parameters('LS_Azure_Sql_DB_properties_typeProperties_server')]"
					},
					"LS_Azure_Sql_DB_properties_typeProperties_database": {
						"value": "[parameters('LS_Azure_Sql_DB_properties_typeProperties_database')]"
					},
					"LS_Azure_Sql_DB_properties_typeProperties_userName": {
						"value": "[parameters('LS_Azure_Sql_DB_properties_typeProperties_userName')]"
					},
					"LS_Http_properties_typeProperties_url": {
						"value": "[parameters('LS_Http_properties_typeProperties_url')]"
					},
					"LS_Rest_properties_typeProperties_url": {
						"value": "[parameters('LS_Rest_properties_typeProperties_url')]"
					},
					"LS_Sql_Db_Az_properties_typeProperties_server": {
						"value": "[parameters('LS_Sql_Db_Az_properties_typeProperties_server')]"
					},
					"LS_Sql_Db_Az_properties_typeProperties_database": {
						"value": "[parameters('LS_Sql_Db_Az_properties_typeProperties_database')]"
					},
					"LS_Sql_Db_Az_properties_typeProperties_userName": {
						"value": "[parameters('LS_Sql_Db_Az_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-mydev-pipeline_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-mydev-pipeline_ArmTemplate_0')]"
			]
		}
	]
}